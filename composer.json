{
    "name": "andreo/eventsauce-outbox",
    "description": "Extended message outbox components for EventSauce.",
    "keywords": [
        "eventsauce",
        "components",
        "outbox",
        "message"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrzej Pakula",
            "email": "andrze.pakula@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "eventsauce/eventsauce": "^3.0",
        "eventsauce/message-outbox-for-doctrine": "^1.0",
        "eventsauce/test-utilities": "^3.0",
        "symfony/console": "^6.2",
        "symfony/dependency-injection": "^6.2"
    },
    "require-dev": {
        "eventsauce/uuid-encoding": "^1.0",
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Andreo\\EventSauce\\Outbox\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "stable"
}
